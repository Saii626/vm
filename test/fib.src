
# Variable decl
	var 	ct
	var 	a
	var 	b
	var 	c
	var 	cmp

# Loop termination counter
	load 		ct 	0

# Initial vals
	load 		a 	0
	load 		b 	1
	
	debug_print	a
	debug_print	b
	
# Main generation loop
loop:
	add 		c 	a 	b
	load 		a 	b
	load 		b 	c
	debug_print	c
	
# Check for loop termination
	add 		ct 	ct 	1
	eq 		cmp 	ct 	15
	cmp_jmp 	cmp 	:end 	:loop

# Complete
end:
	halt
